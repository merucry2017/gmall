<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.merc.gmall.cart.mapper.OmsCartItemMapper">

    <delete id="deleteBySkuIdAndMemberId" parameterType="String">
        DELETE
        FROM
            oms_cart_item
        WHERE
            product_sku_id = #{skuId}
        AND
            member_id = #{memberId}
    </delete>

</mapper>