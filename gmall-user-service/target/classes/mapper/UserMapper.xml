<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.merc.gmall.user.mapper.UserMapper">

    <select id="selectAllUser" resultType="com.merc.gmall.bean.UmsMember">
        select t.* from ums_member t
    </select>

    <select id="findByUsername" resultType="com.merc.gmall.bean.UmsMember">
        select * from ums_member where username=#{username}
    </select>
</mapper>